import java.util.Scanner;

public class BilGanjilUser {
    public static void main(String[] args) {
        System.out.println("BILANGAN GANJIL YANG DITENTUKAN USER");
        
        Scanner input = new Scanner(System.in);
        
        //input bilangan dari user
        System.out.print("Nilai awal bilangan = ");
        int nilaiawal = input.nextInt();
        System.out.print("Nilai akhir bilangan = ");
        int nilaiakhir = input.nextInt();
        
        //apabila nilali awal nya genap maka harus ditambah 1 dulu
        if(nilaiawal % 2 == 0){
            nilaiawal += 1;
            System.out.println("Bilangan ganjil adalah = ");
            for(int i = nilaiawal; i <= nilaiakhir; i+=2){
            System.out.print(i+ " ");
            }    
        }else {
            System.out.print("Bilangan ganjil adalah = ");
            for(int i = nilaiawal; i <= nilaiakhir; i +=2 ){
            System.out.print(i +" ");
            }
        }

        //kuadrat bilangan
        System.out.println(" ");
        //Kuadrat Bilangan
        for(int i = nilaiawal; i<=nilaiakhir; i= i+2){
        int kuadrat;
        kuadrat = i*i;
            System.out.println("Kuadrat dari " +i+ " adalah " +kuadrat);
        }  
    }
}
